{
	"name": "@coderlzw/store",
	"version": "0.0.1",
	"description": "提供基于浏览器的storage、cookie的简易操作，支持storage过期、序列化、返序列化、加密、解密等功能",
	"main": "dist/index.js",
	"types": "dist/types/index.d.ts",
	"author": "梁志伟",
	"license": "ISC",
	"homepage": "https://github.com/coderlzw-cn/web-store#readme",
	"keywords": [
		"cookie",
		"localstorage",
		"sessionstorage"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/coderlzw-cn/web-store"
	},
	"scripts": {
		"build": "rollup -c",
		"lint": "eslint",
		"lint:fix": "eslint --fix",
		"test": "jest",
		"prepare": "npm run build",
		"prepublishOnly": "npm run prepublish",
		"prepublish": "npm run test && npm run lint && npm run build",
		"preversion": "npm run lint:fix && npm run lint",
		"version": "npm run lint:fix && git add -A",
		"postversion": "git push && git push --tags"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"files": [
		"dist",
		"cookies",
		"storage"
	],
	"dependencies": {
		"@rollup/plugin-commonjs": "^26.0.1",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"crypto-js": "^4.2.0",
		"rollup": "^4.20.0",
		"rollup-plugin-cleaner": "^1.0.0",
		"rollup-plugin-typescript2": "^0.36.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.8.0",
		"@types/crypto-js": "^4.2.2",
		"@types/jest": "^29.5.12",
		"eslint": "~9.8.0",
		"globals": "^15.9.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jsdom": "^24.1.1",
		"ts-jest": "^29.2.4",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.0.1"
	}
}
